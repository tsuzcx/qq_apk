<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/g7" />
            <solid android:color="@color/al2" />
            <stroke android:width="1.0px" android:color="@color/x8" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/g7" />
            <solid android:color="@color/x6" />
            <stroke android:width="1.0px" android:color="@color/x9" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/g7" />
            <solid android:color="@color/v9" />
            <stroke android:width="1.0px" android:color="@color/x8" />
        </shape>
    </item>
</selector>