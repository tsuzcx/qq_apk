<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/a0" />
            <solid android:color="@color/a54" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/a0" />
            <stroke android:width="2.0px" android:color="@color/a55" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/a0" />
            <solid android:color="@color/a53" />
            <stroke android:width="2.0px" android:color="@color/a54" />
        </shape>
    </item>
</selector>