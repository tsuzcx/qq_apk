<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fw" />
            <solid android:color="@color/afz" />
            <stroke android:width="1.0px" android:color="@color/u4" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fw" />
            <solid android:color="@color/u2" />
            <stroke android:width="1.0px" android:color="@color/u5" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/fw" />
            <solid android:color="@color/s4" />
            <stroke android:width="1.0px" android:color="@color/u4" />
        </shape>
    </item>
</selector>