<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/e3" />
            <solid android:color="@color/a3x" />
            <stroke android:width="@dimen/e5" android:color="@color/h_" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/e3" />
            <solid android:color="@color/a3x" />
            <stroke android:width="@dimen/e5" android:color="@color/i1" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/e3" />
            <solid android:color="@color/a3x" />
            <stroke android:width="@dimen/e5" android:color="@color/i0" />
        </shape>
    </item>
</selector>