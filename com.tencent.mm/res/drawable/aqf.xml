<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/aa1" />
            <size android:height="@dimen/g" android:width="@dimen/g" />
            <corners android:radius="@dimen/m9" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/aa2" />
            <size android:height="@dimen/g" android:width="@dimen/g" />
            <corners android:radius="@dimen/m9" />
        </shape>
    </item>
</selector>