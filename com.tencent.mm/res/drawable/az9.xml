<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/BW_0_Alpha_0_1" />
            <stroke android:width="0.5dip" android:color="@color/FG_2" />
            <corners android:radius="@dimen/cb" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dip" android:color="@color/FG_2" />
            <solid android:color="@color/ac_" />
            <corners android:radius="@dimen/cb" />
        </shape>
    </item>
</selector>