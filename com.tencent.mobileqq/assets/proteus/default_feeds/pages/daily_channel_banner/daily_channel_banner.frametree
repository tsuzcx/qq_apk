{
    "tag": "page",
    "content": "",
    "attributes": {
        "viewID": "0",
        "pts:on-exposure": "{{$RIJArticle.rowKey}}",
        "data-event-type": "{{'allInOneJump'}}",
        "data-jump-url": "",
        "data-feeds_type": "{{card_report_params.feeds_type}}",
        "data-channel_jump_type": "{{card_report_params.channel_jump_types}}"
    },
    "styles": {
        "flex-direction": "column",
        "display": "flex",
        "box-sizing": "border-box"
    },
    "children": [
        {
            "tag": "view",
            "content": "\n  \n  \n  \n",
