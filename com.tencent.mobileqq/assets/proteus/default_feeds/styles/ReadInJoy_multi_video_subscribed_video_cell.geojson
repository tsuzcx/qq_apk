{
    "ReadInJoy_multi_video_subscribed_video_cell": {
        "view_id": "id_video_cell",
        "view_type": "UIView",
        "height": {
            "type": "wrap_content"
        },
        "width": {
            "type": "wrap_content"
        },
        "default_visible": "YES",
        "event_type":"multi_video_column_video_click",
        "data_attributes": {
            "setRemoteInfo:": {
                "rowkey":"$rowkey"
            }
        },
        "subviews": [
            {
                "view_id": "id_article_large_image_container",
                "view_type": "container",
                "layout_type": "layout_relative",
                "margin_left": "6",
                "height": {
                    "type": "relative",
                    "value": "180"
                },
                "width": {
                    "type": "relative",
                    "value": "320"
                },
                "subviews": [
                    {
                        "view_type": "ReadInjoyAsynImageView",
                        "view_id": "id_video_cover",
                        "height": {
                            "type": "match_parent"
                        },
                        "width": {
                            "type": "match_parent"
                        },
                        "attributes": {
                            "setImageWithURLString:lineup:": [
                                "$video_cover_url",
                                "$lineup"
                            ]
                        }
                    },
                    {
                        "view_type": "UIView",
                        "view_id": "id_article_large_image_cover",
                        "default_visible": "YES",
                        "height": {
                            "type": "match_parent"
                        },
                        "width": {
                            "type": "match_parent"
                        },
                        "attributes": {
                            "setGradientWithStartAlpha:endAlpha:color:direction:": [
                                "0.4",
                                "0",
                                "#000000",
                                "1"
                            ]
                        }
                    },
                    {
                        "view_id": "id_video_title",
                        "view_type": "UILabel",
                        "align_parent_left": "YES",
                        "align_parent_top": "YES",
                        "margin_top": "12.5",
                        "margin_left": "12",
                        "height": {
                            "type": "wrap_content"
                        },
                        "width": {
                            "type": "relative",
                            "value": "296"
                        },
                        "align_parent_center_horizontal": "YES",
                        "attributes": {
                            "setText:": "$video_title_text",
                            "setTextColorString:": "#FFFFFF",
                            "setBoldFontSizeString:": "17",
                            "setNumberOfLinesString:": "2"
                        }
                    },
                    {
                        "view_id": "id_video_info",
                        "view_type": "UILabel",
                        "align_parent_left": "YES",
                        "below_of_view": "id_video_title",
                        "margin_left": "12",
                        "margin_top": "4",
                        "height": {
                            "type": "wrap_content"
                        },
                        "width": {
                            "type": "wrap_content"
                        },
                        "attributes": {
                            "setText:": "$video_info_text",
                            "setTextColorString:": "#FFFFFF",
                            "setFontSizeString:": "10",
                            "setNumberOfLinesString:": "1"
                        }
                    },
                    {
                        "view_id": "id_play_icon",
                        "view_type": "UIImageView",
                        "align_parent_center": "YES",
                        "default_visible": "YES",
                        "height": {
                            "type": "relative",
                            "value": "50"
                        },
                        "width": {
                            "type": "relative",
                            "value": "50"
                        },
                        "attributes": {
                            "loadImageWithPath:": "rij_multi_video_column_play"
                        }
                    }
                ]
            }
        ]
    }
}
