{
    "view_type":"UIView",
    "view_id":"id_comment_expose_subcomment_one_bg",
    "visible_depend_on_children":"YES",
    "event_type":"cmd_secondreplycommment_click",
    "height":{
        "type":"wrap_content"
    },
    "width":{
        "type":"match_parent"
    },
    "attributes":{
        "setBackgroundColors:forStates:":[
            [
                "#00000000",
                "#e5e5e5"
            ],
            [
                "0",
                "1"
            ]
        ]
    },
    "subviews":[
        {
            "margin_top":"10",
            "margin_left":"12",
            "margin_right":"12",
            "margin_bottom":"10",
            "view_type":"RIJCommentRichTextView",
            "view_id":"id_comment_expose_subcomment_one",
            "event_type":"cmd_secondreplycommment_click",
            "height":{
                "type":"wrap_content"
            },
            "width":{
                "type":"match_parent"
            },
            "attributes":{
                "setCommentModel:":"$subcomment_one",
                "setRichData:": [
                    {
                        "name": "div",
                        "attrs": {
                            "class": "div_class",
                            "style": "line-height: 24px; width:351px;color: #aa0000;-webkit-line-clamp:${subcomment_one_text_line}"
                        },
                        "children": [
                            {
                              "name": "a",
                              "attrs": {
                                  "href": "$subcomment_one_nick_url",
                                  "style": "line-height: 24px; color: #376099;font-size:15px",
                                  "ratio": "$subcomment_one_ratio"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "$subcomment_one_nick_name"
                                }
                              ]
                            },
                            {
                                "name": "img",
                                "attrs": {
                                  "style": "width:${subcomment_one_tag_icon_width}; height:15px",
                                  "src": "$subcomment_one_tag_icon_path"
                                }
                            },
                            {
                                "name": "text",
                                "attrs": {
                                    "class": "div_class",
                                    "style": "line-height: 24px; color: #000000;font-size:15px;",
                                    "ratio": "$subcomment_one_ratio"
                                },
                                "children": [
                                    {
                                        "type": "text",
                                        "text": "$subcomment_one_reply"
                                    }
                                ]
                            },
                            {
                              "name": "a",
                              "attrs": {
                                  "href": "$subcomment_one_to_nick_url",
                                  "style": "line-height: 24px; color: #376099;font-size:15px",
                                  "ratio": "$subcomment_one_ratio"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "$subcomment_one_to_nick_name"
                                }
                              ]
                            },
                            {
                                "name": "div",
                                "attrs": {
                                    "class": "div_class",
                                    "style": "line-height: 24px; color: #000000;font-size:15px;",
                                    "ratio": "$subcomment_one_ratio"
                                },
                                "children": "$subcomment_one_rpt_data_info"
                            }
                        ]
                    }
                ]
            }
        },
        {
            "$includeComponet": "componet/RIJ_author_like_sub_one.geojson"
        }
    ]
}
