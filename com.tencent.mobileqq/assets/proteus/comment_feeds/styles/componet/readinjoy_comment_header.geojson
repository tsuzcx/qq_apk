{
    "view_type": "container",
    "layout_type": "layout_relative",
    "default_visible": "YES",
    "view_id": "id_comment_cell_header",
    "margin_top":"12",
    "height": {
        "type": "wrap_content"
    },
    "width": {
        "type": "relative",
        "value": "375"
    },
    "subviews": [
        {
            "view_type": "UIView",
            "layout_type": "layout_relative",
            "default_visible": "YES",
            "view_id": "id_comment_header_component",
            "margin_left": "13",
            "height": {
                "type": "absolutely",
                "value": "49"
            },
            "width": {
                "type": "absolutely",
                "value": "49"
            },
            "subviews": [
                {
                    "view_type": "UIImageView",
                    "view_id": "id_comment_head_borders",
                    "align_parent_center": "YES",
                    "height": {
                        "type": "absolutely",
                        "value": "43"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "43"
                    },
                    "attributes": {
                        "loadImageWithPath:": "$rij_comment_avatar_borders"
                    }
                },
                {
                    "view_type": "PTSAvatarView",
                    "view_id": "id_comment_avator",
                    "default_visible": "YES",
                    "align_parent_center": "YES",
                    "event_type": "cmd_avatar_click",
                    "height": {
                        "type": "absolutely",
                        "value": "37"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "37"
                    },
                    "attributes": {
                        "defaultImage:": "$default_comment_avatar",
                        "loadImageWithPath:": "$avator_url",
                        "setDTElementID:withParams:": [
                            "comment_uin_button",
                            {
                                "rowkey": "$rowkey",
                                "actionid": "click_comment_uin",
                                "click_type": "single",
                                "puin": "$puin",
                                "comment_id": "$comment_id",
                                "sub_comment_id": "$sub_comment_id",
                                "comment_type": "$comment_type"
                            }
                        ]
                    }
                },
                {
                    "view_type": "ReadInjoyAsynImageView",
                    "view_id": "id_comment_head_pendant",
                    "height": {
                        "type": "absolutely",
                        "value": "49"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "49"
                    },
                    "attributes": {
                        "setImageWithURLString:lineup:": [
                            "$rij_comment_avatar_pendant",
                            "-1"
                        ]
                    }
                },
                {
                    "view_type":"UIButton",
                    "default_visible": "NO",
                    "view_id":"id_comment_follow_button",
                    "align_parent_bottom":"YES",
                    "align_parent_center_horizontal":"YES",
                    "setAlphaString:": "1.0",
                    "height":{
                        "type":"absolutely",
                        "value":"18"
                    },
                    "width":{
                        "type":"absolutely",
                        "value":"39"
                    },
                    "event_type":"cmd_follow_button_click",
                    "attributes":{
                        "setSelectedString:":"$follow_btn_show",
                        "setBackgroundImages:forStates:":[
                            [
                                "unfollow_bg",
                                "follow_bg_white"
                            ],
                            [
                                "0",
                                "4"
                            ]
                        ]
                    }
                },
                {
                    "view_type": "container",
                    "view_id": "id_comment_head_v_icon_container",
                    "default_visible":"YES",
                    "height": {
                        "type": "absolutely",
                        "value": "32"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "32"
                    }
                },
                {
                    "view_type": "UIImageView",
                    "view_id": "id_comment_head_v_icon",
                    "below_of_view": "id_comment_head_v_icon_container",
                    "right_of_view": "id_comment_head_v_icon_container",
                    "height": {
                        "type": "absolutely",
                        "value": "14"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "14"
                    },
                    "attributes": {
                        "loadImageWithPath:": "$rij_comment_v"
                    }
                }
            ]
        },
        {
            "view_type": "QQRIJCommentLikeButton",
            "view_id": "id_like_button",
            "default_visible": "YES",
            "event_type": "cmd_like_click",
            "align_parent_right": "YES",
            "margin_right": "16",
            "height": {
                "type": "relative",
                "value": "18"
            },
            "width": {
                "type": "wrap_content"
            },
            "attributes": {
                "setCommentModel:": "$commentModel",
                "setDTElementID:withParams:": [
                    "like_comment_button",
                    {
                        "rowkey": "$rowkey",
                        "actionid": "$comment_like_actionid",
                        "click_type": "single",
                        "comment_id": "$comment_id",
                        "sub_comment_id": "$sub_comment_id",
                        "comment_type": "$comment_type"
                    }
                ]
            }
        },
        {
            "view_type": "UIView",
            "layout_type": "layout_linear",
            "visible_depend_on_children": "YES",
            "view_id": "id_comment_describe_and_comment_container",
            "direction": "vertical",
            "margin_left": "65",
            "height": {
                "type": "wrap_content"
            },
            "width": {
                "type": "wrap_content"
            },
            "subviews": [{
                "view_type": "UIView",
                "layout_type": "layout_linear",
                "visible_depend_on_children": "YES",
                "view_id": "id_comment_header_nickname_container",
                "direction": "horizontal",
                "height": {
                    "type": "wrap_content"
                },
                "width": {
                    "type": "wrap_content"
                },
                "subviews": [{
                    "view_type": "UIButton",
                    "view_id": "id_comment_nickname",
                    "event_type": "cmd_avatar_click",
                    "height": {
                        "type": "relative",
                        "value": "18"
                    },
                    "width": {
                        "type": "wrap_content"
                    },
                    "attributes": {
                        "setTitle:": "$nickname",
                        "setTitleColors:forStates:": [
                            [
                                "#376099",
                                "#50376099"
                            ],
                            [
                                "0",
                                "1"
                            ]
                        ],
                        "setFontSizeString:": "14"
                    }
                },
                {
                    "view_type": "ReadInjoyAsynImageView",
                    "view_id": "id_header_activity_level_icon",
                    "align_parent_center_vertical": "YES",
                    "margin_left": "6",
                    "height": {
                        "type": "absolutely",
                        "value": "15"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "26"
                    },
                    "attributes": {
                        "setImageWithURLString:lineup:": ["$activity_level_icon_url", "1"]
                    }
                },
                {
                    "view_type": "UILabel",
                    "view_id": "id_comment_header_flag_one",
                    "margin_left": "6",
                    "align_parent_center_vertical": "YES",
                    "height": {
                        "type": "absolutely",
                        "value": "15"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "32"
                    },
                    "attributes": {
                        "setTextAlignmentString:": "1",
                        "setText:": "$author_header_flag_one",
                        "setCornerRadiusString:": "7.5",
                        "setTextColorString:": "#006BFF",
                        "setFontSizeString:": "10",
                        "setNumberOfLinesString:": "1",
                        "setBackgroundColorString:":"#1A006BFF"
                    }
                },
                {
                    "view_type": "UILabel",
                    "view_id": "id_comment_header_follow_flag",
                    "margin_left": "6",
                    "align_parent_center_vertical": "YES",
                    "height": {
                        "type": "absolutely",
                        "value": "15"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "51"
                    },
                    "attributes": {
                        "setTextAlignmentString:": "1",
                        "setText:": "$author_header_follow_flag",
                        "setCornerRadiusString:": "7.5",
                        "setTextColorString:": "#4E525E",
                        "setFontSizeString:": "10",
                        "setNumberOfLinesString:": "1",
                        "setBackgroundColorString:":"#F5F5F5"
                    }
                },
                {
                    "view_type": "ReadInjoyAsynImageView",
                    "view_id": "id_header_medal",
                    "event_type": "cmd_header_medal_click",
                    "align_parent_center_vertical": "YES",
                    "margin_left": "6",
                    "height": {
                        "type": "absolutely",
                        "value": "18"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "36"
                    },
                    "attributes": {
                        "setImageWithURLString:lineup:": ["$header_medal_image_url", "1"],
                        "setWHRate:widthBenchmark:": ["$header_medal_wh_rate", "NO"],
                        "setAttatchInfo:": "$header_medal_jump_url"
                    }
                },
                {
                    "view_type": "ReadInjoyAsynImageView",
                    "view_id": "id_header_family_lord",
                    "event_type": "cmd_header_family_lord_click",
                    "align_parent_center_vertical": "YES",
                    "margin_left": "6",
                    "height": {
                        "type": "absolutely",
                        "value": "20"
                    },
                    "width": {
                        "type": "absolutely",
                        "value": "20"
                    },
                    "attributes": {
                        "setImageWithURLString:lineup:": ["$header_family_load_image_url", "1"],
                        "setWHRate:widthBenchmark:": ["$header_family_load_wh_rate", "YES"],
                        "setAttatchInfo:": "$header_family_jump_url"
                    }
                }

                ]
            },
            {
                "view_type": "UILabel",
                "view_id": "id_comment_describe",
                "margin_top": "0.5",
                "height": {
                    "type": "relative",
                    "value": "18"
                },
                "width": {
                    "type": "wrap_content"
                },
                "attributes": {
                    "setText:": "$comment_describe",
                    "setTextColorString:": "#A6A6A6",
                    "setFontSizeString:": "12"
                }
            },
            {
                "view_type": "UIView",
                "view_id": "id_describe_blank",
                "height": {
                    "type": "relative",
                    "value": "2.5"
                },
                "width": {
                    "type": "relative",
                    "value": "5"
                }
            },
            {
                "$includeComponet": "componet/readinjoy_comment_richtext.geojson"
            },
            {
                "$includeComponet": "componet/readinjoy_comment_links.geojson"
            }
            ]
        }
    ]
}
