{
    "ReadInjoy_article_forbid_comment_operate_cell":{
        "view_type":"container",
        "direction":"vertical",
        "layout_type":"layout_relative",
        "height":{
            "type":"relative",
            "value":"46"
        },
        "width":{
            "type":"relative",
            "value":"375"
        },
        "subviews":[
            {
                "view_type":"UIView",
                "view_id":"id_operate_bg",
                "default_visible":"YES",
                "height":{
                    "type":"match_parent"
                },
                "width":{
                    "type":"match_parent"
                },
                "attributes":{
                    "setBackgroundColorString:":"#D9FFFFFF"
                }
            },
            {
                "view_type": "UIView",
                "view_id": "id_label_separator_line_top",
                "default_visible":"YES",
                "margin_left": "0",
                "margin_top": "0",
                "height": {
                    "type": "absolutely",
                    "value": "0.5"
                },
                "width": {
                    "type": "match_parent"
                },
                "attributes":{
                    "setBackgroundColorString:":"#E6E6E6"
                }
            },
            {
                "view_type":"ReadInJoyLikeButton",
                "view_id":"id_like",
                "align_parent_center_vertical":"YES",
                "event_type":"cmd_like_click",
                "default_visible":"YES",
                "height":{
                    "type": "match_parent"
                },
                "width":{
                    "type":"relative",
                    "value":"125"
                },
                "attributes":{
                    "setImages:colors:direction:forStates:":[
                        ["$like_normalstate_image","NativeArticleLikeEnable"],
                        ["#000000","#000000"],
                        ["0","0"],
                        ["0","1"]
                    ],
                    "setDTElementID:withParams:": [
                        "like_button",
                        {
                            "rowkey": "$dt_rowkey",
                            "content_type": "5",
                            "dt_pgid": "1",
                            "actionid": "$dt_like_actionid",
                            "click_type": "single"
                        }
                    ]
                }
            },
            {
                "view_type":"UIView",
                "view_id":"id_separate_line1",
                "right_of_view":"id_like",
                "align_parent_center_vertical":"YES",
                "default_visible":"YES",
                "height":{
                    "type":"relative",
                    "value":"23"
                },
                "width":{
                    "type":"relative",
                    "value":"0.5"
                },
                "attributes":{
                    "setBackgroundColorString:":"#1f000000"
                }
            },
            {
                "view_type":"UIImageView",
                "view_id":"id_operation_coin",
                "right_of_view":"id_separate_line1",
                "default_visible":"YES",
                "margin_left": "40",
                "event_type":"cmd_coin_click",
                "align_parent_center_vertical":"YES",
                "align_parent_center_horizontal":"YES",
                "height":{
                    "type":"relative",
                    "value":"46"
                },
                "width":{
                    "type":"relative",
                    "value":"46"
                },
                "attributes":{
                    "loadImageWithPath:": "$coin_img_status"
                }
            },
            {
                "view_type":"UIView",
                "view_id":"id_separate_line2",
                "right_of_view":"id_operation_coin",
                "align_parent_center_vertical":"YES",
                "default_visible":"YES",
                "margin_left": "40",
                "height":{
                    "type":"relative",
                    "value":"23"
                },
                "width":{
                    "type":"relative",
                    "value":"0.5"
                },
                "attributes":{
                    "setBackgroundColorString:":"#1f000000"
                }
            },
            {
                "view_type":"UIButton",
                "view_id":"id_share",
                "right_of_view":"id_separate_line2",
                "event_type":"cmd_share_click",
                "default_visible":"YES",
                "align_parent_center_vertical":"YES",
                "height":{
                    "type":"relative",
                    "value":"46"
                },
                "width":{
                    "type":"relative",
                    "value":"125"
                },
                "attributes":{
                    "setImage:": "share_bottom_icon",
                    "setDTElementID:withParams:": [
                        "biu_button",
                        {
                            "rowkey": "$dt_rowkey",
                            "content_type": "5",
                            "dt_pgid": "1",
                            "actionid": "click_share",
                            "click_type": "single"
                        }
                    ]
                }
            }
        ]
    }
}
