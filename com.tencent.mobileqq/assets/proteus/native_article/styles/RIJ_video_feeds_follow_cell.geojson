{
    "RIJ_video_feeds_follow_cell":{
        "view_type":"container",
        "direction":"vertical",
        "height":{
            "type":"wrap_content"
        },
        "width":{
            "type":"relative",
            "value":"138"
        },
        "subviews":[
            {
                "view_type":"UIView",
                "view_id":"id_follow_container",
                "layout_type":"layout_linear",
                "default_visible":"YES",
                "direction":"vertical",
                "event_type":"cmd_pack_container_click",
                "margin_right":"8",
                "height":{
                    "type":"relative",
                    "value":"161"
                },
                "width":{
                    "type":"relative",
                    "value":"130"
                },
                "attributes":{
                    "setBackgroundColorString:":"#f7242424",
                    "setCornerRadiusString:":"6.0",
                    "setBorderColorString:":"#4d666666",
                    "setBorderWidthString:":"0.5"
                },
                "subviews":[
                    {
                        "view_id":"id_follow_info_avator_container",
                        "view_type":"container",
                        "layout_type":"layout_relative",
                        "margin_top":"13",
                        "align_parent_center_horizontal":"YES",
                        "height":{
                            "type":"relative",
                            "value":"60"
                        },
                        "width":{
                            "type":"relative",
                            "value":"60"
                        },
                        "subviews":[
                            {
                                "view_type":"UIImageView",
                                "view_id":"id_follow_info_avator_mask",
                                "default_visible":"YES",
                                "height":{
                                    "type":"relative",
                                    "value":"60"
                                },
                                "width":{
                                    "type":"relative",
                                    "value":"60"
                                },
                                "attributes":{
                                    "loadImageWithPath:":"follow_info_avator_mask"
                                }
                            },
                            {
                                "view_type":"ReadInJoyAvatarView",
                                "view_id":"id_follow_info_avator",
                                "align_parent_center_horizontal":"YES",
                                "align_parent_center_vertical":"YES",
                                "event_type":"cmd_pack_avatar_click",
                                "default_visible":"YES",
                                "height":{
                                    "type":"relative",
                                    "value":"57"
                                },
                                "width":{
                                    "type":"relative",
                                    "value":"57"
                                },
                                "attributes":{
                                    "loadAvatarByUin:":"$avatar_uin"
                                }
                            }
                        ]
                    },
                    {
                        "view_type":"UILabel",
                        "view_id":"id_author_name",
                        "margin_top":"4",
                        "align_parent_center_horizontal":"YES",
                        "height":{
                            "type":"wrap_content"
                        },
                        "width":{
                            "type":"wrap_content"
                        },
                        "attributes":{
                            "setTextColorString:":"#FFFFFF",
                            "setText:":"$author_name",
                            "setMaxWidth:type:":[
                                "110",
                                "relative"
                            ],
                            "setNumberOfLinesString:":"1",
                            "setFontSizeString:":"16"
                        }
                    },
                    {
                        "view_type":"UILabel",
                        "view_id":"id_author_desc",
                        "margin_top":"3",
                        "align_parent_center_horizontal":"YES",
                        "height":{
                            "type":"wrap_content"
                        },
                        "width":{
                            "type":"wrap_content"
                        },
                        "attributes":{
                            "setTextColorString:":"#AAAAAA",
                            "setText:":"$author_desc",
                            "setMaxWidth:type:":[
                                "110",
                                "relative"
                            ],
                            "setNumberOfLinesString:":"1",
                            "setFontSizeString:":"11"
                        }
                    },
                    {
                        "view_type":"UIButton",
                        "view_id":"id_follow_button",
                        "align_parent_center_horizontal":"YES",
                        "margin_top":"5",
                        "margin_bottom":"12",
                        "height":{
                            "type":"relative",
                            "value":"24"
                        },
                        "width":{
                            "type":"relative",
                            "value":"54"
                        },
                        "event_type":"cmd_pack_follow_click",
                        "attributes":{
                            "setSelectedString:":"$selected_status",
                            "setBackgroundImages:forStates:":[
                                [
                                    "NativeArticleHeaderUnfollowed",
                                    "NativeArticleHeaderFollowed"
                                ],
                                [
                                    "0",
                                    "4"
                                ]
                            ]
                        }
                    }
                ]
            }
        ]
    }
}