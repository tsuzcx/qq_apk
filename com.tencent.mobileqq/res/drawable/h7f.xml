<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="4.0dip" />
            <gradient android:angle="45.0" android:centerY="0.75" android:color="@color/ajr" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <size android:height="4.0dip" />
                <gradient android:startColor="@color/ajr" android:endColor="@color/ajr" android:angle="45.0" android:centerY="0.75" android:centerColor="@color/ajr" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="4.0dip" />
                <gradient android:startColor="@color/al3" android:endColor="@color/al3" android:angle="45.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>