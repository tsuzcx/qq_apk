<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#aa6666ff" />
            <corners android:topLeftRadius="@dimen/ne" android:topRightRadius="@dimen/ne" android:bottomLeftRadius="@dimen/ne" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ff6666ff" />
            <corners android:topLeftRadius="@dimen/ne" android:topRightRadius="@dimen/ne" android:bottomLeftRadius="@dimen/ne" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
</selector>