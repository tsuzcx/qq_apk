        <shape>
            <solid android:color="#806f7172" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#d0ffffff" />
                <corners android:radius="5.0dip" />
