<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/al3" />
            <stroke android:width="1.0dip" android:color="@color/a_7" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/al3" />
            <stroke android:width="1.0dip" android:color="@color/a_8" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/al3" />
            <stroke android:width="1.0dip" android:color="@color/a_8" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
</selector>