<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <stroke android:width="0.0dip" android:color="#ff93939d" />
            <solid android:color="#ff93939d" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="2.0dip" />
                <stroke android:width="0.0dip" android:color="#7bffffff" />
                <solid android:color="#7bffffff" />
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="2.0dip" />
                <stroke android:width="0.0dip" android:color="#ff00cfff" />
                <solid android:color="#ff00cfff" />
            </shape>
        </scale>
    </item>
</layer-list>