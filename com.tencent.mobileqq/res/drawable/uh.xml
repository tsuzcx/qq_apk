<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:topLeftRadius="1.0px" android:topRightRadius="1.0px" android:bottomLeftRadius="1.0px" android:bottomRightRadius="1.0px" />
            <solid android:color="#ffe4e7f4" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:topLeftRadius="1.0px" android:topRightRadius="1.0px" android:bottomLeftRadius="1.0px" android:bottomRightRadius="1.0px" />
                <solid android:color="#ff00cafc" />
            </shape>
        </clip>
    </item>
</layer-list>