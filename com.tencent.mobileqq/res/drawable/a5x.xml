<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="#ffdedfe0" />
            <gradient android:startColor="#ffeeefef" android:endColor="#ffeeefef" android:angle="270.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffeeefef" android:endColor="#ffeeefef" android:angle="270.0" />
            <stroke android:width="1.0px" android:color="#ffdedfe0" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" />
            <stroke android:width="1.0px" android:color="#ffdedfe0" />
        </shape>
    </item>
</selector>