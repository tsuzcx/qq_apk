<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#4d969fa9" android:endColor="#4d969fa9" android:angle="270.0" android:centerY="0.75" android:centerColor="#4d969fa9" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#fff83e50" android:endColor="#fff83e50" android:angle="270.0" android:centerY="0.75" android:centerColor="#fff83e50" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff1aa1e6" android:endColor="#ff1aa1e6" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff1aa1e6" />
            </shape>
        </clip>
    </item>
</layer-list>