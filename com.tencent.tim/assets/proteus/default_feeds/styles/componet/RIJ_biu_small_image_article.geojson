{
    "view_type":"UIView",
    "view_id":"id_middle_body_container",
    "layout_type":"layout_relative",
    "margin_left":"12",
    "visible_depend_on_children": "YES",
    "margin_right":"12",
    "margin_bottom":"10",
    "height":{
        "type":"relative",
        "value":"72"
    },
    "width":{
        "type":"match_parent"
    },
    "attributes":{
        "setBackgroundColorString:":"#F5F6FA"
    },
    "subviews":[
        {
            "view_type":"UIView",
            "view_id": "id_image_container",
            "visible_depend_on_children":"YES",
            "layout_type":"layout_relative",
            "align_parent_right":"YES",
            "width": {
                "type":"wrap_content"
            },
            "height": {
                "type":"wrap_content"
            },
            "subviews":[
                {
                    "view_type":"ReadInjoyAsynImageView",
                    "view_id":"id_middle_content_cover_img",
                    "height":{
                        "type":"relative",
                        "value":"72"
                    },
                    "width":{
                        "type":"relative",
                        "value":"113"
                    },
                    "attributes":{
                        "setImageWithURLString:lineup:":[
                            "$middle_content_cover_img_url",
                            "-1"
                        ]
                    }
                },
                {
                    "view_type":"UIImageView",
                    "view_id":"id_middle_content_cover_default_img",
                    "height":{
                        "type":"relative",
                        "value":"72"
                    },
                    "width":{
                        "type":"relative",
                        "value":"113"
                    },
                    "attributes":{
                        "loadImageWithPath:": "$middle_content_cover_default_img"
                    }
                }
            ]
        },
        {
            "view_type":"UILabel",
            "view_id":"id_middle_content_txt",
            "align_parent_center_vertical":"YES",
            "align_parent_left":"YES",
            "left_of_view":"id_image_container",
            "margin_left":"12",
            "margin_right":"12",
            "height":{
                "type":"wrap_content"
            },
            "width":{
                "type":"match_parent"
            },
            "attributes":{
                "setText:":"$middle_content_txt",
                "setTextColorString:":"#242424",
                "setFontSizeString:":"16",
                "setNumberOfLinesString:":"2"
            }
        }
    ]
}
